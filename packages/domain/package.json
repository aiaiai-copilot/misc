{
  "name": "@misc-poc/domain",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "test -d src && yarn exec tsc --build || echo 'No src directory found, skipping build'",
    "test": "jest",
    "lint": "test -d src && eslint src --ext .ts || echo 'No src directory found, skipping lint'",
    "typecheck": "yarn exec tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@misc-poc/shared": "workspace:*",
    "remove-accents": "^0.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.0.0",
    "jest-util": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.9.2"
  }
}
