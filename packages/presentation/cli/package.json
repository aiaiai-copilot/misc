{
  "name": "@misc-poc/presentation-cli",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "bin": {
    "misc-poc": "./dist/index.js"
  },
  "scripts": {
    "build": "test -d src && yarn exec tsc --build || echo 'No src directory found, skipping build'",
    "test": "jest",
    "lint": "test -d src && eslint src --ext .ts || echo 'No src directory found, skipping lint'",
    "typecheck": "yarn exec tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@misc-poc/application": "workspace:*",
    "@misc-poc/domain": "workspace:*",
    "@misc-poc/shared": "workspace:*",
    "commander": "^11.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "jest-util": "^29.0.0",
    "ts-jest": "^29.0.0"
  }
}
